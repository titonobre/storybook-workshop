{
  "name": "storybook-workshop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev --host localhost --port 6006 --no-open",
    "build-storybook": "storybook build",
    "watch-storybook": "nodemon --watch .storybook --ext js,ts --exec 'npm run storybook'",
    "test-storybook": "test-storybook",
    "report-coverage": "npx nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-a11y": "^7.0.0-beta.46",
    "@storybook/addon-actions": "^7.0.0-beta.46",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^7.0.0-beta.46",
    "@storybook/addon-interactions": "^7.0.0-beta.46",
    "@storybook/addon-links": "^7.0.0-beta.46",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/react": "^7.0.0-beta.46",
    "@storybook/react-vite": "^7.0.0-beta.46",
    "@storybook/test-runner": "^0.10.0-next.8",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "axe-playwright": "^1.2.1",
    "babel-loader": "^9.1.2",
    "chromatic": "^6.17.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "storybook": "^7.0.0-beta.46",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  }
}
